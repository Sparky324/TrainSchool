<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Math.Class8.Theme3.MathClass8Theme3Activity">

    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/toTasksMath71"
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:onClick="onToTasksMath71Click"
                android:text="Теория прочитана! К задачам" />

            <TextView
                android:id="@+id/textView61"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Квадрат суммы"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />

            <TextView
                android:id="@+id/textView62"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Квадрат разности"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />

            <TextView
                android:id="@+id/textView63"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Разность квадратов"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />

            <TextView
                android:id="@+id/textView64"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Сумма кубов"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />

            <TextView
                android:id="@+id/textView65"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Разность кубов"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />

            <TextView
                android:id="@+id/textView66"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Куб суммы"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />

            <TextView
                android:id="@+id/textView67"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Куб разности"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ru.noties.jlatexmath.JLatexMathView
                android:id="@+id/j_latex_math_view6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="16dip"
                app:jlmv_alignHorizontal="center"
                app:jlmv_alignVertical="center"
                app:jlmv_textSize="16sp" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>